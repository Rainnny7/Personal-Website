{
  "name": "personal-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss --input src/styles/globals.css --output src/app/globals.css --minify --watch\"",
    "build": "tailwindcss src/styles/globals.css --output src/app/globals.css --minify && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "concurrently": "^7.5.0",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  }
}
